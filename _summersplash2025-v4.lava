<!-- End of Events -->

<!-- Render Captured Events -->
<section class="summersplash25-section-blue">
    <div class="container">
        <div class="row mt-1">
            <div class="col-sm-12">
                <!-- intro -->
                <div class="text-center mt-5 twmc-mbm summersplash25-may py-3">
                    <h1 class="twmc-h1 summersplash25-section-title text-uppercase">&nbsp;</h1>
                </div>

                <!-- events -->
                <div class="summersplash25-flex-container">
                {{ MonthMay }}
                </div>
            </div>
        </div>

    </div>
</section>

